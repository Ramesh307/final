@model IEnumerable<SchoolManageMentSystem.Models.LeaveModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Lstatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ldescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Schedule)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StudentId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Lstatus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ldescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Schedule)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StudentId)
            </td>
            <td>
                <a href = "/Leave/Edit?id=@item.Id">Edit</a>
                 <a href = "/Leave/Delete?id=@item.Id">Delete</a>
              
            </td>
        </tr>
}
    </tbody>
</table>
